<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sibetech.core.mapper.BizMapper">
    <select id="queryList" resultType="cn.sibetech.core.domain.Biz">
        select t.* from p_biz t
        where  1 = 1
        order by orderby
    </select>
</mapper>
